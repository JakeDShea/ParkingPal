<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:id="@+id/frame_layout"
        android:layout_width="371dp"
        android:layout_height="417dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="12dp"
        android:layout_marginTop="0dp" >

    </FrameLayout>

    <Button
        android:id="@+id/kaplanButton"
        android:layout_width="136dp"
        android:layout_height="70dp"
        android:layout_below="@+id/frame_layout"
        android:layout_alignStart="@+id/frame_layout"
        android:layout_alignEnd="@+id/frame_layout"
        android:layout_marginStart="-4dp"
        android:layout_marginTop="3dp"
        android:layout_marginEnd="238dp"
        android:text="Kaplan Parking" />

    <Button
        android:id="@+id/parkingDeckButton"
        android:layout_width="136dp"
        android:layout_height="70dp"
        android:layout_alignStart="@+id/frame_layout"
        android:layout_alignTop="@+id/frame_layout"
        android:layout_alignEnd="@+id/frame_layout"
        android:layout_marginStart="261dp"
        android:layout_marginTop="420dp"
        android:layout_marginEnd="-2dp"
        android:text="Parking Deck" />

    <Button
        android:id="@+id/setParkingLocation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/frame_layout"
        android:layout_alignEnd="@+id/frame_layout"
        android:layout_alignBottom="@+id/kaplanButton"
        android:layout_marginStart="153dp"
        android:layout_marginEnd="124dp"
        android:layout_marginBottom="1dp"
        android:text="Set Parking" />


    <TextView
        android:id="@+id/destinationAddress"
        android:layout_width="match_parent"
        android:layout_height="25dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="1dp"
        android:layout_marginTop="527dp"
        android:layout_marginEnd="-1dp"
        android:layout_marginBottom="51dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/parkingAddress"
        android:layout_width="405dp"
        android:layout_height="24dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="3dp"
        android:layout_marginTop="495dp"
        android:layout_marginEnd="3dp"
        android:layout_marginBottom="84dp"
        android:text="TextView" />


</RelativeLayout>